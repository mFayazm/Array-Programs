import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int [] a = new int[5];
        
        System.out.println("Enter numbers:");
        
        for(int i=0;i<a.length;i++){
            a[i] = sc.nextInt();
        }
        System.out.print("Array" + Arrays.toString(a));
    }
}
