public class Main{
    public static void main(String[]args){
        int [] a = {15,20,30,11,4};
        
        int min = a[0];
        
        for(int i=0;i<a.length;i++){
            if(a[i]<min){
                min = a[i];
                
            }
        }
        System.out.println("Minimum Element: " + min);
    }
}
