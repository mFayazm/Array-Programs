public class Main{
    public static void main(String[] args){
        int[] a = {10,15,20,40,59};
        int sum = 0;
        
        for(int i=0;i<a.length;i++){
            sum += a[i];
            System.out.println(a[i]);
        }
        System.out.println("Sum of elements=" + sum);
    }
}
