public class Main{
    public static void main(String[] args)
    {
        int[] a = {10,20,30,40,50};
        
        System.out.println("Elements at even indices:");
        
        for(int i=0;i<a.length;i+=2)
        {
            System.out.println("Index " + i + ":" + a[i]);
        }
    }
}
