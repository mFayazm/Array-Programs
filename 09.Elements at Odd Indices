public class Main{
    public static void main(String[] args)
    {
        int[] a = {10,20,30,40,50,60,70};
        
        System.out.println("Elements at Odd Indices:");
        
        for(int i=1;i<a.length;i+=2)
        {
            System.out.println("Index " + i + ":" + a[i]);
        }
    }
}
