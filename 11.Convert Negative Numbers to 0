import java.util.Arrays;

public class Main{
    public static void main(String[] args){
        int[] a = {11,-7,-17,26,-6,90,100,66,-12};
        
        for(int i=0;i<a.length;i++){
            if(a[i]<0){
                a[i]=0;
            }
        }
        System.out.println("Converted Array: " + Arrays.toString(a));
    }
}
